

public class Server extends Thread{
	private int idx;
	private int myId;
	private int nextId;
	private int prevId;
	
	public Server(int idx, int myId,int nextId,int prevId){
		this.idx = idx;
		this.myId = myId;
		this.nextId = nextId;
		this.prevId = prevId;
	}
	
	public void run() {
		System.out.println("Hello from a thread!" + myId);
	}

}

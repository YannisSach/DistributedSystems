import java.net.*;
import java.io.*;

public class Server extends Thread{
	public int idx;
	public int myId;
	public int nextId;
	public int prevId;
	
	public Server(int idx, int myId,int nextId,int prevId){
		this.idx = idx;
		this.myId = myId;
		this.nextId = nextId;
		this.prevId = prevId;
	}
	
	public void run() {
		System.out.println("Hello from a thread!" + myId);
		Socket MyClient;
	    try {
	           MyClient = new Socket("localhost", 25000);
	    }
	    catch (IOException e) {
	        System.out.println(e);
	    }
	}

}

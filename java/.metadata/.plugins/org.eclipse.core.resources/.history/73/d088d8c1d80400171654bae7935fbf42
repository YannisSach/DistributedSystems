import java.net.*;
import java.io.*;


public class MySocket{
	public Socket myClient;
	
	public MySocket(int port){
		Socket MyClient=null;
	    try {
	           MyClient = new Socket("localhost", 25000);
	    }
	    catch (IOException e) {
	        System.out.println(e);
	    }
	    this.myClient = myClient;
		
	}

}

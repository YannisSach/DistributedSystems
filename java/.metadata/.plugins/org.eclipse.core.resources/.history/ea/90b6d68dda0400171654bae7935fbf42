import java.net.*;
import java.io.*;


public class DHT {
	
	public static void main (String[] args){
		ServerSocket serv=null;
		try {
			serv = new ServerSocket(25000);
		}
		catch (IOException e) {
	           System.out.println(e);
	           
	    }
		Server mys = new Server(1,1,1,1);
		mys.start();
		
		Socket serviceSocket = null;
	    try {
	       serviceSocket = serv.accept();
	        }
	    catch (IOException e) {
	       System.out.println(e);
	    }
	    
	    MySocket sock = new MySocket(serviceSocket);
	    
	    
	       System.out.println(sock.read());
	    

		
	}

}

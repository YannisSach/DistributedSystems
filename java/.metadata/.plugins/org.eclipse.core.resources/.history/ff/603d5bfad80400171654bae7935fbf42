import java.net.*;
import java.io.*;


public class MySocket{
	public Socket mySocket;
	public int myPort;
	
	public MySocket(int port){
		this.myPort = port;
		Socket MyClient=null;
	    try {
	           MyClient = new Socket("localhost", 25000);
	    }
	    catch (IOException e) {
	        System.out.println(e);
	    }
	    this.mySocket = MyClient;
		
	}

}

import java.util.concurrent.locks.*;

public class Util {
	
	public Lock l = new Lock();
	public static int port = 10000;
	public static boolean debug = true;
	
	
	
	
	public static void debug(String msg){
		if (debug)
			System.out.println(msg);
	}
	public static int getPort(){
		int tempPort = port++;
		return tempPort;
	}
	
	
}

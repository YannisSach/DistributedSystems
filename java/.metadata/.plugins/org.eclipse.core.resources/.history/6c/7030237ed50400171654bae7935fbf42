import java.net.*;

public class Server extends Thread{
	public int idx;
	public int myId;
	public int nextId;
	public int prevId;
	public Socket 
	
	public Server(int idx, int myId,int nextId,int prevId){
		this.idx = idx;
		this.myId = myId;
		this.nextId = nextId;
		this.prevId = prevId;
	}
	
	public void run() {
		System.out.println("Hello from a thread!" + myId);
	}

}
